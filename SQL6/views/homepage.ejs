<div class="container" id="customers">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-6 map">
                <!-- <div id="floating-panel">
                    <input type="text" name="inputText">
                </div> -->
                <div id="map"></div>
            </div>
            <div class="col-md-6">

                <form role="form">
                    <div class="form-group">
                        <label for="state">Countries</label>
                        <select type="text" class="form-control" id="country" name="country" onchange="onCountrySelection()">
                            <option value="">Select a Country</option>
                            <% for(var i = 0; i < countries.length; i++) { %>
                            <option value="<%= countries[i].Country %>"><%= countries[i].Country %></option>
                            <% } %>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="state">State</label>
                        <select type="text" class="form-control" id="state" name="state" onchange="onStateSelection()">
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="city">City:</label>
                        <select type="text" class="form-control" id="city" name="city" onchange="hotelQuery()">
                        </select>
                    </div>
                </form>

                <!-- <table id="customerTable" class="table table-striped">
                    <thead>
                    <tr>
                        <th>Code</th>
                        <th>Customer</th>
                    </tr>
                    </thead>
                    <tbody>
                    <% for(var i = 0; i < customers.length; i++) { %>
                    <tr>
                        <td><%= customers[i].CustomerCode %></td>
                        <td><%= customers[i].CustomerName %></td>
                    </tr>
                    <% } %>
                    </tbody>
                </table> -->
            </div>
        </div>
    </div>
</div>

<br/>
